{
    "$schema": "https://typedoc.org/schema.json",
    "includeVersion": true,
    "entryPoints": ["lib/typedoc.index.ts"],
    "plugin": [
        "typedoc-umlclass",
        "typedoc-plugin-mdn-links",
        "typedoc-plugin-missing-exports"
    ],
    "umlClassDiagram": {
        "type": "detailed",
        "location": "local",
        "format": "png",
        "legend": "only-included",
        "style": {
            "backgroundColor": "white"
        }
    },
    "placeInternalsInOwningModule": true,
}
