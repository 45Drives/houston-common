    "@heroicons/vue": "^2.1.3",
    "deep-equal": "^2.2.3",
    "monet": "^0.9.3",
    "neverthrow": "6.2.2",
    "three": "^0.173.0",
    "uuid": "^9.0.1",
    "vue": "^3.5.13",
    "zod": "^3.23.8",
    "zod-validation-error": "^3.3.0"
  },
  "devDependencies": {
    "@fontsource/red-hat-text": "^5.0.18",
