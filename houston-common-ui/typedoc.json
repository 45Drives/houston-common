{
    "includeVersion": true,
    "entryPoints": ["dist/index.d.ts"],
    "tsconfig": "tsconfig.typedoc.json",
    "plugin": [
        "typedoc-umlclass",
        "typedoc-plugin-mdn-links",
        "typedoc-plugin-vue",
        "typedoc-plugin-missing-exports"
    ],
    "umlClassDiagram": {
        "type": "none",
        "location": "local",
        "format": "png",
        "legend": "only-included",
        "style": {
            "backgroundColor": "white"
        }
    },
    "compilerOptions": {
        "skipLibCheck": true,
    },
    "placeInternalsInOwningModule": true,
}
